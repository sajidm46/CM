@model  MailgunAPIDirect.Entities.EventStats
<div class="css-sm-table-TagStatus" style="">
    @if (Model != null)
    {
        <div class="table-header">
            <div class="row">
                <div class="col-sm-7 campaign-manager float-left "><label>@Model.tag</label></div>
            </div>
        </div>
        <div class="table-body">
            <br />
            <div class="row">
                <div class="col-sm-2 campaign-manager float-left ">@Html.Label("Accepted", new { id = "lblTagName", @class = "css-sm-bold" })</div>
                <div class="col-sm-10 campaign-manager float-left light-grey ">Mailgun accepted the request to send/forward the email and the message has been placed in queue.</div>
            </div>
            <div class="row">
                <div class="col-sm-2 campaign-manager float-left "></div>
                <div class="col-sm-2 campaign-manager float-left "></div>
                <div class="col-sm-2 campaign-manager float-left "></div>
                <div class="col-sm-2 campaign-manager float-left ">Incoming: @Model.Accepted.Incoming</div>
                <div class="col-sm-2 campaign-manager float-left ">Outgoing: @Model.Accepted.Outgoing</div>
                <div class="col-sm-2 campaign-manager float-left ">Total: @Model.Accepted.Total</div>
            </div>
            <div class="row">
                <div class="col-sm-2 campaign-manager float-left ">@Html.Label("Delivered", new { id = "lblDelivered", @class = "css-sm-bold" })</div>
                <div class="col-sm-10 campaign-manager float-left  light-grey">Mailgun sent the email and it was accepted by the recipient email server.</div>
            </div>
            <div class="row">
                <div class="col-sm-2 campaign-manager float-left "></div>
                <div class="col-sm-2 campaign-manager float-left "></div>
                <div class="col-sm-2 campaign-manager float-left "></div>
                <div class="col-sm-2 campaign-manager float-left ">Http: @Model.Delivered.Http</div>
                <div class="col-sm-2 campaign-manager float-left ">Smtp: @Model.Delivered.Smtp</div>
                <div class="col-sm-2 campaign-manager float-left ">Total: @Model.Delivered.Total</div>
            </div>
            <div class="row">
                <div class="col-sm-2 campaign-manager float-left ">@Html.Label("Bounced", new { id = "lblBounced", @class = "css-sm-bold" })</div>
                <div class="col-sm-10 campaign-manager float-left  light-grey">Mailgun could not deliver the email to the recipient email server.</div>
            </div>
            <!--<div class="row">
             <div class="col-sm-2 campaign-manager float-left "></div>
             <div class="col-sm-2 campaign-manager float-left ">Bounce: @Model.Failed.Permanent.Bounce</div>
             <div class="col-sm-2 campaign-manager float-left ">Delayed Bounce: @Model.Failed.Permanent.DelayedBounce</div>
             <div class="col-sm-3 campaign-manager float-left ">Suppress Bounce: @Model.Failed.Permanent.SuppressBounce</div>
            </div>-->
            <div class="row">
                <div class="col-sm-2 campaign-manager float-left "></div>
                <!--<div class="col-sm-3 campaign-manager float-left ">Suppress Complaint:@Model.Failed.Permanent.SuppressComplaint</div>
                <div class="col-sm-3 campaign-manager float-left ">Suppress Unsubscribe: @Model.Failed.Permanent.SuppressUnsubscribe</div>-->
                <div class="col-sm-2 campaign-manager float-left "></div>
                <div class="col-sm-2 campaign-manager float-left "></div>
                <div class="col-sm-2 campaign-manager float-left "></div>
                <div class="col-sm-2 campaign-manager float-left "></div>
                <div class="col-sm-2 campaign-manager float-left ">Total: @Model.Failed.Permanent.Total</div>
            </div>
            <div class="row">
                <div class="col-sm-2 campaign-manager float-left ">@Html.Label("Opened", new { id = "lblOpened", @class = "css-sm-bold" })</div>
                <div class="col-sm-10 campaign-manager float-left  light-grey">The email recipient opened the email and enabled image viewing.</div>
            </div>
            <div class="row">
                <div class="col-sm-2 campaign-manager float-left "></div>
                <div class="col-sm-2 campaign-manager float-left "></div>
                <div class="col-sm-2 campaign-manager float-left "></div>
                <div class="col-sm-2 campaign-manager float-left "></div>
                <div class="col-sm-2 campaign-manager float-left ">Unique: @Model.Opened.Unique</div>
                <div class="col-sm-2 campaign-manager float-left ">Total: @Model.Opened.Total</div>
            </div>

            <div class="row">
                <div class="col-sm-2 campaign-manager float-left ">@Html.Label("stored", new { id = "lblStored", @class = "css-sm-bold" })</div>
                <div class="col-sm-10 campaign-manager float-left  light-grey">Mailgun has stored an incoming message.</div>
            </div>
            <div class="row">
                <div class="col-sm-2 campaign-manager float-left "></div>
                <div class="col-sm-2 campaign-manager float-left "></div>
                <div class="col-sm-2 campaign-manager float-left "></div>
                <div class="col-sm-2 campaign-manager float-left "></div>
                <div class="col-sm-2 campaign-manager float-left "></div>
                <div class="col-sm-2 campaign-manager float-left ">Total: @Model.Opened.Total</div>
            </div>



            <div class="row">
                <div class="col-sm-2 campaign-manager float-left ">@Html.Label("Clicked", new { id = "lblClicked", @class = "css-sm-bold" })</div>
                <div class="col-sm-10 campaign-manager float-left  light-grey">The email recipient clicked on a link in the email.</div>
            </div>
            <div class="row">
                <div class="col-sm-2 campaign-manager float-left "></div>
                <div class="col-sm-2 campaign-manager float-left "></div>
                <div class="col-sm-2 campaign-manager float-left "></div>
                <div class="col-sm-2 campaign-manager float-left "></div>
                <div class="col-sm-2 campaign-manager float-left ">Unique: @Model.Clicked.Unique</div>
                <div class="col-sm-2 campaign-manager float-left ">Total: @Model.Clicked.Total</div>
            </div>
            <div class="row">
                <div class="col-sm-2 campaign-manager float-left ">@Html.Label("Complained", new { id = "lblComplained", @class = "css-sm-bold" })</div>
                <div class="col-sm-10 campaign-manager float-left  light-grey">The email recipient clicked on the spam complaint button within their email client.</div>
            </div>
            <div class="row">
                <div class="col-sm-2 campaign-manager float-left "></div>
                <div class="col-sm-2 campaign-manager float-left "></div>
                <div class="col-sm-2 campaign-manager float-left "></div>
                <div class="col-sm-2 campaign-manager float-left "></div>
                <div class="col-sm-2 campaign-manager float-left "></div>
                <div class="col-sm-2 campaign-manager float-left ">Total: @Model.Complained.Total</div>
            </div>
            <div class="row">
                <div class="col-sm-2 campaign-manager float-left ">@Html.Label("Unsubscribed", new { id = "lblUnsubscribed", @class = "css-sm-bold" })</div>
                <div class="col-sm-10 campaign-manager float-left  light-grey">The email recipient clicked on the unsubscribe link.</div>
            </div>
            <div class="row">
                <div class="col-sm-2 campaign-manager float-left "></div>
                <div class="col-sm-2 campaign-manager float-left "></div>
                <div class="col-sm-2 campaign-manager float-left "></div>
                <div class="col-sm-2 campaign-manager float-left "></div>
                <div class="col-sm-2 campaign-manager float-left "></div>
                <div class="col-sm-2 campaign-manager float-left ">Total: @Model.Unsubscribed.Total</div>
            </div>
            <div class="row">
                <div class="col-sm-2  campaign-manager float-left ">@Html.Label("Failed Temporary", new { id = "lblFailedTemporary", @class = "css-sm-bold" })</div>
                <div class="col-sm-10 campaign-manager float-left  light-grey">Mailgun could not deliver the email to the recipient email server.</div>
            </div>
            <div class="row">
                <div class="col-sm-2 campaign-manager float-left "></div>
                <div class="col-sm-2 campaign-manager float-left "></div>
                <div class="col-sm-2 campaign-manager float-left "></div>
                <div class="col-sm-2 campaign-manager float-left "></div>
                <div class="col-sm-2 campaign-manager float-left "></div>
                <!--<div class="col-sm-2 campaign-manager float-left ">Espblock: @Model.Failed.Temporary.Espblock</div>-->
                <div class="col-sm-2 campaign-manager float-left ">Total: @Model.Failed.Temporary.Total</div>
            </div>

        </div>
    }
</div>
<br /><br />
      <div>
          <a class="btn btn-lg btn-info" href="/Tag/Index">Back</a>
          <a class="btn btn-lg btn-warning" href="/Tag/Delete">Delete</a>
      </div>

